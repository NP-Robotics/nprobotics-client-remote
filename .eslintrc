{
  "extends": ["airbnb", "airbnb/hooks"],
  "rules": {
    "no-unused-vars": [1],
    "no-underscore-dangle": [0],
    "jsx-a11y/media-has-caption": [0],
    "no-use-before-define": [0],
    "no-lonely-if": [1],
    "react-hooks/exhaustive-deps": [0],
    "react/no-array-index-key": [0]
  }, 
  "env": {
    "browser": true,
    "node": true
  }
    
}
